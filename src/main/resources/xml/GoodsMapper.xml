<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zzh.mybatisplusdaynamicannotationsql.mapper.GoodsMapper">
    <update id="upMoney">
        update goods
        set price = price - 10
        where id = 30
    </update>
    <update id="downMoney">
        update goods
        set price = price + 10
        where id = 31
    </update>
</mapper>
